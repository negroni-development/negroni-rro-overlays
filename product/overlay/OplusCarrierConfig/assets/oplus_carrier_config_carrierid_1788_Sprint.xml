<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<carrier_config>
    <!-- #ifdef link.zhang@NETWORK, 2020/09/19, [ITN-63593] disable some mmi codes for both USS and UST -->
    <string-array name="mmi_two_digit_number_pattern_string_array" num="7">
        <item value="0"/>
        <item value="1"/>
        <item value="00"/>
        <item value="*2"/>
        <item value="*3"/>
        <item value="*4"/>
        <item value="*5"/>
    </string-array>
    <!-- #endif end -->
    <!-- #ifdef [IKR-55249]Add by tyler.song for VoLTE Network Preference Wireframe Addition @20210127-->
    <int name="carrier_default_wfc_ims_roaming_mode_int" value="2" />
    <int name="carrier_default_wfc_ims_mode_int" value="1" />
    <!-- #endif end  [IKR-55249]Add by tyler.song for VoLTE Network Preference Wireframe Addition @20210127-->
    <!-- boolean name="rtt_supported_bool" value="true" /-->
    <!-- boolean name="carrier_rtt_upgrade_supported" value="true"/-->
    <!-- boolean name="carrier_rtt_downgrade_supported" value="true"/-->
    <!-- boolean name="carrier_volte_available_bool" value="true" /-->
    <!-- boolean name="carrier_wfc_ims_available_bool" value="true" /-->
    <boolean name="carrier_rtt_auto_upgrade" value="true" />
    <!-- #ifdef joker.bian_2019-4-2 turn on RTT for USS-->
    <boolean name="rtt_supported_bool" value="true" />
    <boolean name="carrier_rtt_supported_on_vtcalls" value="true"/>
    <boolean name="carrier_rtt_upgrade_supported" value="true"/>
    <boolean name="carrier_rtt_downgrade_supported" value="true"/>
    <!-- #endif end-->
    <!-- #ifdef joker.bian_2019-5-10 use own number for vm-->
    <boolean name="config_telephony_use_own_number_for_voicemail_bool" value="true"/>
    <!-- #endif end-->
    <!-- #ifdef kai.yin_2019-3-28 turn on VoLTE and WFC for Sprint-->
    <boolean name="carrier_volte_available_bool" value="true" />
    <boolean name="carrier_wfc_ims_available_bool" value="true" />
    <!-- #endif end -->
    <!-- #ifdef kai.yin_2019-05-29 show default VoLTE settings to OFF -->
    <boolean name="enhanced_4g_lte_on_by_default_bool" value="false" />
    <!-- #endif end -->
    <!-- #ifdef 2019-5-7 not allow nonEmergency call in ecm-->
    <boolean name="allow_non_emergency_calls_in_ecm_bool" value="false" />
    <!-- #endif end -->
    <!-- #ifdef 2019-4-17 show HD icon for Sprint-->
    <boolean name="identify_high_definition_calls_in_call_log_bool" value="true" />
    <!-- #endif end -->
    <!-- #ifdef 2019-5-5 GCES-2169 show alert when open tty in volte/wfc-->
    <boolean name="carrier_volte_tty_supported_bool" value="false" />
    <!-- #endif end -->
    <!-- #ifdef 2019-5-7, for GCES-2211 -->
    <boolean name="vendor_ecc_after_radio_on_under_flight_mode" value="true" />
    <!-- #endif end -->
    <!-- #ifdef kai.yin 20190802 Not turn off IMS service completely-->
    <boolean name="carrier_allow_turnoff_ims_bool" value="false" />
    <!-- #endif end-->
    <boolean name="config_oplus_show_5g_pref" value="true" />
    <!-- #ifdef jackson.yu_20210902 show iccid in sim status-->
    <boolean name="show_iccid_in_sim_status_bool" value="true" />
    <!-- #endif end -->
</carrier_config>
